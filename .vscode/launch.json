{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "protocol": "inspector",
      "program": "${workspaceRoot}/build/index.js"
    },

    {
      "name": "Debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/ts-node/dist/bin.js",
      "args": ["${workspaceRoot}/src/index.ts"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector"
    }
  ]
}
